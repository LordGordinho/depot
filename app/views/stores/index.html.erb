<% if notice %>
  <aside id="notice"><%= notice %></aside>
<% end %>

<h1>Your Pragmatic Catalog</h1>

<%= turbo_stream_from 'products' %>

<ul class="catalog" >
  <% cache @products do %>
    <% @products.each do |product| %>
      <% cache product do %>
        <%= render partial: 'product', object: product %>
      <% end %>
    <% end %>
  <% end %>
</ul>